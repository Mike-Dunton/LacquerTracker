<% include ../layoutTop %>

<!-- content -->
<div class="wrapper row2">
    <div id="container" class="clear">
    	<section>
    		<div>
	     		<br><h3>Combine Duplicate Polishes:</h3><br>
                <% for (i=0; i<polishes.length; i++) { %>
                    <a href="/polishid/<%=polishes[i].id%>"><%=polishes[i].name%></a> - <%=polishes[i].brand%> - <%=polishes[i].id%> - <%=polishes[i].code%> - <%=polishes[i].colorcat%> - <%=polishes[i].type%> - <%=polishes[i].batch%> - <%=polishes[i].indie%> - <%=polishes[i].dupes%> - <%=polishes[i].swatch%> - <a href="/polishid/<%=polishes[i].id%>/delete">delete!</a>
                    <br><br>
                <% } %>
                <br><br>
                <form action="/admin/duplicate/remove" method="post">
                    Id to Keep: <input name="keepid" class="width50" required><br><br>
                    Id to Remove: <input name="removeid" class="width50" required><br><br>
                    <input type="submit" value="Submit" class="button">
                </form>
            </div>
    	</section>
    </div>
</div>

<% include ../layoutBottom %>